<config>
    <modules>
        <Navoq_Backup>
            <version>1.0.0.0</version>
        </Navoq_Backup>
    </modules>
    <global>
        <helpers>
            <navoq_backup>
                <class>Navoq_Backup_Helper</class>
            </navoq_backup>
        </helpers>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <oauth before="Mage_Adminhtml">Navoq_Backup_Adminhtml</oauth>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <backup>
                    <file>navoq/backup.xml</file>
                </backup>
            </updates>
        </layout>
    </adminhtml>
</config>
